{
  "name": "fimports",
  "displayName": "fimports",
  "description": "This extension autocompletes the Path on imports",
  "icon": "images/logo.png",
  "version": "0.0.24",
  "publisher": "kidandcat",
  "engines": {
    "vscode": "^1.20.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "main": "./out/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.37",
    "tslint": "^5.8.0",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/kidandcat/fimports"
  },
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Fimports configuration",
      "properties": {
        "fimports.sourceCodeFolder": {
          "type": "string",
          "default": "src",
          "description": "Source folder to look for imports."
        }
      }
    }
  }
}
